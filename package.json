{
  "name": "@compeon/timepicker",
  "version": "0.0.6",
  "description": "Simple timepicker using react",
  "main": "dist/timepicker.umd.js",
  "module": "dist/timepicker.es.js",
  "license": "MIT",
  "keywords": [
    "timepicker",
    "react",
    "react-timepicker"
  ],
  "files": [
    "dist"
  ],
  "authors": [
    "Bastian Ahrens <bahrens@compeon.de>",
    "Gerrit Seger <gseger@compeon.de>"
  ],
  "scripts": {
    "compile": "rollup -c",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "prepublishOnly": "yarn test && yarn compile"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "styled-components": "^3.2.6"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/react": "^6.0.19",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest": "^27.0.4",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^2.15.0",
    "rollup-plugin-babel": "^4.3.3",
    "styled-components": "^4.4.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestPlugins.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.js"
    ]
  }
}
